{
  "name": "starter",
  "version": "1.0.0",
  "description": "Natours backend API server",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run watch:js\" \"nodemon server.js\"",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Jenny",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.16.0",
    "mongoose": "^8.18.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "pug": "^3.0.3",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "stripe": "^19.3.0",
    "validator": "^13.15.15",
    "xss-clean": "^0.1.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "ndb": "^0.2.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
